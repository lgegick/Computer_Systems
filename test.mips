.data
    hello_str: .asciiz "Hello, World!\n"

.text
main:
    li      $v0, 4          # 4 is the syscall to print_str
    la      $a0, hello_str  # Load string address into $a0
    syscall

    li      $a0, 0          # set exit code
    li      $v0, 10         # 10 is the syscall to exit
    syscall