{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "dashmips",
      "request": "launch",
      "name": "Dashmips (Run)",
      "program": "${file}"
    },
    {
      "type": "dashmips",
      "request": "attach",
      "name": "Dashmips (Attach)",
      "registerFormat": "dec",
      "host": "localhost",
      "port": 2390,
      "stopOnEntry": false
    },

    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "c:/Users/LRex4/Computer Systems",
      "program": "c:/Users/LRex4/Computer Systems/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}